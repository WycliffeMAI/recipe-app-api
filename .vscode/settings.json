{
    "files.autoSave": "onFocusChange",
    "window.zoomLevel": 0.5,
    "pyhton.languageServer": "pylance",

        "version": "0.2.0",
        "configurations": [
          {
            "name": "Extension",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
              "--extensionDevelopmentPath=${workspaceFolder}"
            ]
          }
        ],
    "emmet.includeLanguages": {
      "django-html": "html"
    }, "emmet.triggerExpansionOnTab": true

}
